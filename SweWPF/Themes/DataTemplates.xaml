<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:SweWPF.ViewModels"
    xmlns:vw="clr-namespace:SweWPF.Views"
    >
    
    <DataTemplate DataType="{x:Type vm:InputViewModel}">
        <vw:InputView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CalculationResultViewModel}">
        <vw:ResultView />
    </DataTemplate>

</ResourceDictionary>